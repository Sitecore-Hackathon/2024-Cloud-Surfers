# Copy these to .env.local and set values

# XM Cloud Authoring endpoint
GRAPHQL_ENDPOINT= "https://YOUR-XMCLOUD.sitecorecloud.io/sitecore/api/authoring/graphql/v1"

# Authoring security key
# Get your key from deploy.sitecorecloud.io deployment details screen
XM_Cloud_Access_Token= "Bearer ey...PASTE-YOUR-KEY-HERE"


# =========================================================================
#  Next Auth
# =========================================================================
# https://next-auth.js.org/
NEXTAUTH_URL=http://localhost:3000

# -------------------------------
# Optional, connect to a microsoft Entra (formerly Azure AD) SSO app
AZURE_AD_CLIENT_ID=
AZURE_AD_CLIENT_SECRET=
AZURE_AD_TENANT_ID=
# Do not track secrets in repos. Add this to .env.local
NEXTAUTH_JWT_SECRET=
